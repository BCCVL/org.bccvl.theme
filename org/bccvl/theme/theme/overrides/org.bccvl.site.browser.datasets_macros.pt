<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      i18n:domain="plone.z3cform"
      tal:omit-tag="">

  <head></head>

  <body>

    <metal:block metal:define-macro="datasets_tabs">

    <ul class="nav nav-tabs">
        <li class="tab-datasetslisting">
            <a href="#"
               tal:define="selected python: view.__name__ == 'datasets_listing_view'"
               tal:attributes="href string:${context/absolute_url};
                               class python: 'active' if selected else None"><span>My Datasets</span></a>
        </li>
        <li class="tab-datasetsimport">
            <a href="#"
               tal:define="selected python: view.__name__ == 'datasets_import_view'"
               tal:attributes="href string:${context/absolute_url}/datasets_import_view;
                               class python: 'active' if selected else None"><span>Import Dataset</span></a>
        </li>
        <li class="tab-datasetsupload">
            <a href="#"
               tal:define="selected python: view.__name__ == 'datasets_upload_view'"
               tal:attributes="href string:${context/absolute_url}/datasets_upload_view;
                               class python: 'active' if selected else None"><span>Upload Dataset</span></a>
        </li>
    </ul>

    </metal:block>
  </body>
</html>
